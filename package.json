{
  "name": "elb-check",
  "version": "1.0.0",
  "description": "Simple static site to test load balancer implementation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "static": "echo Opening static client-side version && start static-client.html",
    "simple": "echo Opening simple hostname version && start simple-hostname.html",
    "build": "node build.js",
